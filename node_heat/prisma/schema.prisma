datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id         String @id @default(uuid()) // campo | tipo do campo | primary key | padr√£o do campo uuid e incremental
  name       String
  github_id  Int
  avatar_url String
  login      String

  @@map("users")
}
